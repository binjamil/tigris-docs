curl 'http://localhost:8081/v1/databases/catalogdb/collections/catalog/documents/replace' \
  -X PUT \
  -H "Content-Type: application/json" \
  -d '{
  "documents":[
    {
      "id":3,
      "name":"sling bag",
      "price":99,
      "brand":"coach",
      "labels":"purses",
      "popularity":9
    }
  ]
}'
